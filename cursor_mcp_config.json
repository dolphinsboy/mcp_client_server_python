{
  "mcpServers": {
    "weather-server": {
      "command": "python",
      "args": ["weather.py", "--port", "8123"],
      "cwd": "./server",
      "env": {},
      "transport": {
        "type": "stdio"
      }
    },
    "weather-http-server": {
      "transport": {
        "type": "streamable-http",
        "url": "http://localhost:8123/mcp"
      }
    }
  }
} 